{
  "name": "yaml-blaster",
  "version": "1.0.20",
  "description": "Framework for pre-processing yaml files",
  "main": "dist/blaster.js",
  "scripts": {
    "test": "npm run build && npx jest",
    "clean": "rm -rf dist",
    "shrink": "npm prune --production && npm shrinkwrap",
    "build": "npx webpack --mode production && sh scripts/add_hashbang.sh",
    "preversion": "npm run clean && npm test",
    "postversion": "git add . && git commit --amend && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molesoft/yaml-blaster.git"
  },
  "author": "Adam Monica",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/molesoft/yaml-blaster/issues"
  },
  "bin": {
    "yaml-blaster": "./dist/cli.js"
  },
  "homepage": "https://github.com/molesoft/yaml-blaster#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "jest": "^26.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "boxen": "^4.0.0",
    "chalk": "^2.4.2",
    "js-yaml": "^3.13.1",
    "lodash.get": "^4.4.2",
    "lodash.uniq": "^4.5.0",
    "uuid": "^8.1.0",
    "yargs": "^13.2.4"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "yaml",
    "aws",
    "cloudformation",
    "parser",
    "load",
    "file",
    "modular",
    "replace",
    "resolve",
    "variables"
  ]
}
